{
  "name": "parse-server-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node server/index.js",
    "pretest": "MONGODB_VERSION=${MONGODB_VERSION:=3.2.6} MONGODB_STORAGE_ENGINE=mmapv1 mongodb-runner start --port 27018",
    "test": "jest",
    "precommit": "lint-staged --verbose",
    "format": "prettier --single-quote --trailing-comma all '{server,src,__{tests,mocks}__}/**/*.js' --write",
    "lint-staged": "lint-staged --verbose",
    "posttest": "MONGODB_VERSION=${MONGODB_VERSION:=3.2.6} MONGODB_STORAGE_ENGINE=mmapv1 mongodb-runner stop",
    "validate:schema": "babel-node server/modules/schemas/validate"
  },
  "author": "bob61016@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "parse-dashboard": "^1.0.25",
    "parse-server": "^2.3.8",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.1",
    "mongodb-runner": "^3.4.0",
    "prettier": "^1.3.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn run format",
      "git add"
    ]
  }
}
