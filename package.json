{
  "name": "parse-server-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node server/index.js",
    "precommit": "lint-staged --verbose",
    "format": "prettier --single-quote --trailing-comma all '{server,src,__{tests,mocks}__}/**/*.{js,jsx}' --write",
    "lint-staged": "lint-staged --verbose",
    "dev": "nodemon server/index.js",
    "test": "jest --onlyChanged --watch",
    "test:all": "jest",
    "validate:schema": "babel-node server/modules/schemas/validate"
  },
  "author": "bob61016@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "parse": "^1.9.2",
    "parse-dashboard": "^1.0.26",
    "parse-server": "^2.4.2",
    "ramda": "^0.24.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^3.6.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.3.1"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/env-setup.js",
      "<rootDir>/__tests__/index.spec.js",
      "<rootDir>/node_modules/"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn run format",
      "git add"
    ]
  }
}
