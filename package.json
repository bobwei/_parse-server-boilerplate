{
  "name": "parse-server-boilerplate",
  "version": "1.1.0",
  "description": "",
  "scripts": {
    "start": "node server/index.js",
    "precommit": "lint-staged --verbose",
    "format": "prettier --single-quote --trailing-comma all '{server,src,__{tests,mocks}__}/**/*.{js,jsx}' --write",
    "lint-staged": "lint-staged --verbose",
    "dev": "nodemon server/index.js",
    "test": "jest --onlyChanged --watch",
    "test:all": "jest",
    "validate:schema": "babel-node server/modules/schemas/validate"
  },
  "author": "bob61016@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "parse": "^1.10.0",
    "parse-dashboard": "^1.1.0",
    "parse-server": "^2.6.3",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/env-setup.js",
      "<rootDir>/__tests__/index.spec.js",
      "<rootDir>/node_modules/"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn run format",
      "git add"
    ]
  }
}
